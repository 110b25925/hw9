# hw9
package hw9_3.java;
class Caaa
{
	private int value;
	
	public Caaa()
	{
		value = 10;
		System.out.println("constructor Crectangle( value = 10) called"); // (a)
		System.out.println("value=" + value);
		
	}
	public Caaa(int i )
	{
		value= i;
		System.out.println("value="+value);
	}
}


public class hw {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Caaa obj1= new Caaa();
		Caaa obj2 = new Caaa(12);

	}

}




// (b) public 與 private 乎叫
// (C) 不行 因為 PUBLIC 要在變數之前
// (d) 不會 因為
